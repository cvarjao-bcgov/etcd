{
  "kind": "BuildConfig",
    "apiVersion": "v1",
    "metadata": {
      "name": "csnr-etcd" 
    },
  "spec": {
    "triggers": [
      {
        "type": "GitHub",
        "github": {
          "secret": "secret101"
        }
      },
      {
        "type": "Generic",
        "generic": {
          "secret": "secret101"
        }
      },
      {
        "type": "ImageChange"
      }
    ],
    "source": { 
      "type": "Git",
      "git": {
        "uri": "git://github.com/openshift/ruby-hello-world.git"
      }
    },
    "strategy": { 
      "type": "Source",
      "sourceStrategy": {
        "from": {
          "kind": "ImageStreamTag",
          "name": "ruby-20-centos7:latest"
        }
      }
    },
    "output": { 
      "to": {
        "kind": "ImageStreamTag",
        "name": "csnr-etcd:latest"
      }
    }
  }
}